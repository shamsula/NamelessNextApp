{
  "name": "shamsul-demo",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@material-ui/core": "^4.11.0",
    "@mui/material": "^5.10.7",
    "@react-hook/window-size": "^3.1.1",
    "@reduxjs/toolkit": "^1.5.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^17.0.1",
    "styled-components": "^5.2.0",
    "touch-cli": "^0.0.1",
    "ts-jest": "^26.5.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "react-scripts start",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.ts ",
    "start": "npm-run-all --parallel start:*",
    "start:server": "react-scripts start",
    "start:client": "serve --no-clipboard --single --listen 8080 dist",
    "validate": "yarn run test:coverage && yarn run test:e2e:run",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "babel-plugin-macros": "^3.0.1",
    "cypress": "^7.4.0",
    "eslint-plugin-cypress": "^2.11.2",
    "husky": "^6.0.0",
    "is-ci-cli": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "p5": "^1.3.1",
    "pretty-quick": "^3.1.0",
    "react-p5-wrapper": "^2.4.0",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.12.5",
    "swiper": "^8.4.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  }
}
